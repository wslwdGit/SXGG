    <select id="FindAccount" parameterType="java.util.Map"  resultMap="AccountMap">
      	SELECT
			concat( 'Mr.', account.first_name, ' ', last_name ) AS NAME,
			account.birth_date,
			account.email,
			account.check_email,
			account.mobile_phone_number,
			account.Add_time,
			account.agent_id,
			account.commission,
			city_test.en_name,
			account.platform,
			account.id
		FROM
			account
			INNER JOIN city_test ON account.place_of_birth = city_test.CODE 
    </select>
  
  
  
